{
  "name": "@eway-crm/connector",
  "version": "1.0.53",
  "description": "eWay-CRM API JavaScript connector library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eway-crm/js-lib.git"
  },
  "keywords": [
    "eway-crm",
    "eway",
    "crm",
    "connector",
    "api"
  ],
  "author": "eWay System s.r.o.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eway-crm/js-lib/issues"
  },
  "homepage": "https://github.com/eway-crm/js-lib#readme",
  "devDependencies": {
    "@eway-crm/eslint-config": "^1.0.6",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "base64url": "^3.0.1",
    "es6-promise": "^4.2.8",
    "jwt-decode": "^3.1.2"
  }
}
